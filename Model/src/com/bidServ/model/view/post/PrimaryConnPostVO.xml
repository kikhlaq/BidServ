       Post.MODIFIED_DATE,    
       Post.LANGUAGE,    
       Post.OBJECT_VERSION_NUMBER,    
       Post.CHANGE_LOG,    
       Connection.SOURCE_COMPANY_ID SOURCE_COMPANY_ID,    
       Connection.CONNECTION_ID,    
       if(DATEDIFF(DELIVERY_DATE,CURDATE())<0,0,DATEDIFF(DELIVERY_DATE,CURDATE()))  VIEW_ATTR,    
       Company.LOGO_URL,    
       Company.COMPANY_ID  Company_Id1,   
       Company.IS_VERIFIED Is_Verified 
FROM  posts Post   
INNER JOIN Connections Connection ON (Post.COMPANY_ID = Connection.DEST_COMPANY_ID AND Connection.connection_status_code = 'APPROVED') 
INNER JOIN Company_info Company ON (Post.COMPANY_ID = Company.COMPANY_ID AND Company.STATUS = 'APPROVED')  
WHERE Post.VISIBILITY_CODE = 'PRIMARY' 
UNION  
SELECT Post.POST_ID,    
